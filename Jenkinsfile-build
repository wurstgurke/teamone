pipeline {
    agent {
        docker {
            image 'hub.docker.com/wurstgurke/teamone'
        }
    }
    environment {
        HOME="${env.WORKSPACE}"
    }
    stages {
        stage('Build Backend') {
            steps {
                dir('backend'){
                    sh 'mvn clean install'
                }
            }
        }
    }
}
