#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  "$basedir/node"  "$basedir/../@bahmutov/print-env/bin/has-env.js" "$@"
  ret=$?
else 
  node  "$basedir/../@bahmutov/print-env/bin/has-env.js" "$@"
  ret=$?
fi
exit $ret
