{
  "name": "mocha-junit-reporters",
  "version": "1.23.6",
  "description": "A JUnit reporter for mocha.",
  "main": "index.js",
  "scripts": {
    "lint": "./node_modules/.bin/eslint index.js test/helpers/*; exit 0",
    "test": "MOCHA_FILE=test/mocha.xml node_modules/.bin/mocha test --reporter=spec",
    "tdd": "MOCHA_FILE=test/mocha.xml node_modules/.bin/mocha test --reporter=min --watch",
    "release": "standard-version",
    "deploy:prepare": "./scripts/create_npmrc_file.sh"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/you54f/mocha-junit-reporters"
  },
  "keywords": [
    "mocha",
    "junit",
    "reporter"
  ],
  "author": {
    "name": "Michael Allen",
    "email": "michael.lee.allen@gmail.com"
  },
  "contributors": [
    {
      "name": "YOU54F",
      "email": "yousafn@gmail.com"
    }
  ],
  "license": "MIT",
  "devDependencies": {
    "@commitlint/cli": "8.0.0",
    "@commitlint/config-conventional": "8.0.0",
    "chai": "^4.2.0",
    "chai-xml": "^0.3.2",
    "eslint": "^5.16.0",
    "husky": "2.3.0",
    "libxmljs": "^0.19.5",
    "mocha": "^6.1.4",
    "standard-version": "6.0.1",
    "test-console": "^1.1.0"
  },
  "dependencies": {
    "debug": "^4.1.1",
    "md5": "^2.2.1",
    "mkdirp": "~0.5.1",
    "strip-ansi": "^5.2.0",
    "xml": "^1.0.1"
  },
  "peerDependencies": {
    "mocha": ">=6"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  }
}
